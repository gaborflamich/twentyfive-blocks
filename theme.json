{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "styles": {
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--aboreto)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--poppins)"
    }
  },
  "settings": {
    "appearanceTools": true,
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Aboreto",
          "name": "Aboreto",
          "slug": "aboreto",
          "fontFace": [
            {
              "fontFamily": "Aboreto",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/Aboreto-Regular.woff2"]
            }
          ]
        },
        {
          "fontFamily": "Poppins",
          "name": "Poppins",
          "slug": "poppins",
          "fontFace": [
            {
              "fontFamily": "Poppins",
              "fontWeight": "400",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/Poppins-Regular.woff2"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "400",
              "fontStyle": "italic",
              "src": ["file:./assets/fonts/Poppins-Italic.woff2"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "700",
              "fontStyle": "normal",
              "src": ["file:./assets/fonts/Poppins-Bold.woff2"]
            },
            {
              "fontFamily": "Poppins",
              "fontWeight": "700",
              "fontStyle": "italic",
              "src": ["file:./assets/fonts/Poppins-BoldItalic.woff2"]
            }
          ]
        }
      ]
    },
    "color": {
      "palette": [
        {
          "color": "#FFFFFF",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#111111",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#064e3b",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "color": "#facc15",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "color": "#FFEE58",
          "name": "Accent 1",
          "slug": "accent-1"
        },
        {
          "color": "#F6CFF4",
          "name": "Accent 2",
          "slug": "accent-2"
        },
        {
          "color": "#503AA8",
          "name": "Accent 3",
          "slug": "accent-3"
        },
        {
          "color": "#686868",
          "name": "Accent 4",
          "slug": "accent-4"
        },
        {
          "color": "#FBFAF3",
          "name": "Accent 5",
          "slug": "accent-5"
        },
        {
          "color": "color-mix(in srgb, currentColor 20%, transparent)",
          "name": "Accent 6",
          "slug": "accent-6"
        }
      ]
    },
    "layout": {
      "contentSize": "900px",
      "wideSize": "1200px"
    }
  }
}
